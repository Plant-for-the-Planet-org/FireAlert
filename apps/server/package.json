{
  "name": "@firealert/server",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "scripts": {
    "build": "yarn db:deploy && dotenv -e ../../.env next build",
    "dev": "dotenv -e ../../.env next dev",
    "postinstall": "cd ../../ && npx prisma generate",
    "lint": "dotenv -e ../../.env next lint",
    "start": "dotenv -e ../../.env next start",
    "heroku-start": "next start -p $PORT",
    "db:migrate": "dotenv -e ../../.env npx prisma migrate dev --name note-entity --create-only && npx prisma generate",
    "db:push": "dotenv -e ../../.env npx prisma db push",
    "db:pull": "dotenv -e ../../.env npx prisma db pull",
    "db:deploy": "dotenv -e ../../.env npx prisma migrate deploy",
    "db:reset": "dotenv -e ../../.env npx prisma migrate reset",
    "db:generate": "dotenv -e ../../.env npx prisma generate"
  },
  "dependencies": {
    "@logtail/node": "^0.4.0",
    "@planet-sdk/common": "^0.1.11",
    "@prisma/client": "^4.15.0",
    "@sentry/nextjs": "^7.51.2",
    "@sentry/profiling-node": "^0.3.0",
    "@tanstack/react-query": "^4.20.2",
    "@trpc/client": "^10.9.0",
    "@trpc/next": "^10.9.0",
    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "@turf/turf": "^6.5.0",
    "@types/md5": "^2.3.2",
    "@types/nodemailer": "^6.4.7",
    "auth0": "^3.3.0",
    "csv-parse": "^5.3.9",
    "geo-tz": "^7.0.7",
    "mapbox-gl": "1.13",
    "maplibre-gl": "^3.1.0",
    "md5": "^2.3.0",
    "next": "^13.2.1",
    "nodemailer": "^6.9.1",
    "pg-promise": "^11.4.3",
    "phone": "^3.1.37",
    "prisma": "^4.15.0",
    "react-cookie": "^4.1.1",
    "react-map-gl": "7.1.0-beta.3",
    "styled-components": "^6.0.1",
    "superjson": "1.9.1",
    "twilio": "^4.11.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-next": "^13.2.1",
    "prisma": "^4.15.0",
    "tsconfig": "*",
    "typescript": "^5.0.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.8.0"
  }
}
