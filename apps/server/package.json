{
  "name": "@firealert/server",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "schema": "./prisma/schema.prisma"
  },
  "scripts": {
    "build": "yarn db:deploy && dotenv -e ../../.env next build",
    "dev": "dotenv -e ../../.env next dev",
    "postinstall": "cd ../../ && npx prisma generate",
    "lint": "dotenv -e ../../.env next lint",
    "start": "dotenv -e ../../.env next start",
    "heroku-start": "next start -p $PORT",
    "db:migrate": "dotenv -e ../../.env npx prisma migrate dev",
    "db:push": "dotenv -e ../../.env npx prisma db push",
    "db:pull": "dotenv -e ../../.env npx prisma db pull",
    "db:deploy": "dotenv -e ../../.env npx prisma migrate deploy",
    "db:reset": "dotenv -e ../../.env npx prisma migrate reset",
    "db:generate": "dotenv -e ../../.env npx prisma generate",
    "db:studio": "dotenv -e ../../.env npx prisma studio"
  },
  "dependencies": {
    "@google/earthengine": "^0.1.396",
    "@logtail/node": "^0.4.0",
    "@planet-sdk/common": "^0.1.11",
    "@prisma/client": "^5.0.0",
    "@sentry/nextjs": "^10.25.0",
    "@tanstack/react-query": "^4.20.2",
    "@trpc/client": "^10.9.0",
    "@trpc/next": "^10.40.0",
    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "@types/md5": "^2.3.2",
    "@types/nodemailer": "^6.4.7",
    "auth0": "^3.3.0",
    "csv-parse": "^5.3.9",
    "date-fns": "^4.1.0",
    "geo-tz": "^7.0.7",
    "hash-wasm": "^4.9.0",
    "i18n-iso-countries": "^7.14.0",
    "libphonenumber-js": "^1.10.56",
    "mapbox-gl": "1.13",
    "maplibre-gl": "^3.1.0",
    "moment": "^2.30.1",
    "next": "^15.1.0",
    "nodemailer": "^6.9.1",
    "pg-promise": "^11.4.3",
    "phone": "^3.1.37",
    "postcss": "^8.5.6",
    "prisma": "^5.10.2",
    "react": "^19.1.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^19.1.0",
    "react-lottie": "^1.2.3",
    "react-map-gl": "7.1.0-beta.3",
    "superjson": "1.9.1",
    "twilio": "^4.11.0",
    "validator": "^13.9.0",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.14.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-lottie": "^1.2.6",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.34.0",
    "eslint-config-next": "^15.1.0",
    "postcss": "^8.5.6",
    "prisma": "^5.10.2",
    "tailwindcss": "^3",
    "tsconfig": "*",
    "typescript": "^5.0.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.8.0"
  }
}
